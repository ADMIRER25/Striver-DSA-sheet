T.C-->O(N)
S.C-->O(1)
......................................................
class Solution {
public:
    int maxProfit(vector<int>& prices)
    {
       int profit = 0,bday = prices[0];

       for(int i = 1; i < prices.size();i++)
       {
           if(bday > prices[i]) bday = prices[i];
           else profit = max(profit,prices[i]-bday);
       }
       return profit;
    }
};
...........................................................
